package com.java.practice;
class First{
	public void show(){
		System.out.println("Show method ");
	}
}
class Second extends First{
	public void show(){
		System.out.println("Second");
	}
}
public class InhDemo {
	
	public static void main(String[] args) {
		Second obj=new Second();
		obj.show();
	}

}
